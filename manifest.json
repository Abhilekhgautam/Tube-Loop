{
    "manifest_version":3,
    "version":"0.1",
    "name":"TubeLoop",
    "host_permissions": [
        "*://*.youtube.com/"
      ],
      "action": {
        "default_title": "TubeLoop",
        "default_popup": "popup/index.html"
      },
      "content_scripts": [
        {
	  "matches": ["<all_urls>"],
          "js": ["main.js"]
        }
      ]
  }
